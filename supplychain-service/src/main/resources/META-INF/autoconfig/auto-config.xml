        <property name="com.sfebiz.supplychian.oss.warter.position" description="海尚汇水印图片的坐标" required="true"/>
        <property name="com.sfebiz.supplychian.oss.warter.transparency" description="海尚汇水印图片的透明度" required="true"/>
        <property name="com.sfebiz.supplychian.oss.warter.xAxis" description="海尚汇水印图片的水平偏移" required="true"/>
        <property name="com.sfebiz.supplychian.oss.warter.yAxis" description="海尚汇水印图片的垂直偏移" required="true"/>

