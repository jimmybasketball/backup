        <property name="com.sfebiz.supplychian.redisproxy.minIdle" description="最小闲置连接数，spring启动的时候自动建立该数目的连接供应用程序使用，不够的时候会申请。默认值为0" required="true"/>
        <property name="com.sfebiz.supplychian.redisproxy.timeout" description="读取redis的最长超时时间,单位为毫秒" required="true"/>
