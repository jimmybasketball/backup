<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-4.0.xsd"
       default-lazy-init="false">


    <!-- 物流平台与外部系统交互的命令配置 -->
    <bean id="commandConfig" class="com.sfebiz.supplychain.provider.command.common.CommandConfig">
        <property name="apiUrl" value="${com.sfebiz.supplychain.apiurl}"/>
        <property name="fileUrl" value="${com.sfebiz.supplychain.fileurl}"/>
        <property name="printPath" value="${com.sfebiz.supplychain.printpath}"/>
        <property name="lpcMapping">
            <map>
                <entry key="0">
                    <map>
                        <entry key="supplychain.event.wms.skustockin">
                            <map>
                                <entry key="service_name" value="supplychain.event.wms.skustockin"/>
                                <entry key="msg_type" value="LOGISTICS_SKU_STOCKIN_INFO"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.wms.common.WmsSkuStockInCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>


                <!--快递100命令集合-->
                <entry key="16">
                    <map>
                        <entry key="logistics.event.kd100.registroutes">
                            <map>
                                <entry key="service_name" value="logistics.event.kd100.registroutes"/>
                                <entry key="msg_type" value=""/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.tpl.kd100.KD100RegistRoutesCommand"/>
                            </map>
                        </entry>
                        <entry key="logistics.event.kd100.queryroutes">
                            <map>
                                <entry key="service_name" value="logistics.event.kd100.queryroutes"/>
                                <entry key="msg_type" value=""/>
                                <entry key="class_name"
                                       value="com.sfebiz.logistics.provider.command.send.tpl.kd.KD100QueryRouteCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 发网云仓命令集合 -->
                <entry key="34">
                    <map>
                        <entry key="logistics.event.wms.skusync">
                            <map>
                                <entry key="service_name" value="logistics.event.wms.skusync"/>
                                <entry key="msg_type" value="logistics.event.wms.skusync"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.wms.fineex.FineExSkuSyncCommand"/>
                            </map>
                        </entry>
                        <entry key="logistics.event.wms.skustockin">
                            <map>
                                <entry key="service_name" value="logistics.event.wms.skustockin"/>
                                <entry key="msg_type" value="logistics.event.wms.skustockin"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.wms.fineex.FineExSkuStockInCommand"/>
                            </map>
                        </entry>
                        <entry key="logistics.event.wms.create">
                            <map>
                                <entry key="service_name" value="logistics.event.wms.create"/>
                                <entry key="msg_type" value="logistics.event.wms.create"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.wms.fineex.FineExOrderCreateCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>
            </map>
        </property>

        <!-- 口岸commond配置 -->
        <property name="portMapping">
            <map>
                <!-- 杭州电子口岸 -->
                <entry key="HZPORT">
                    <map>
                        <!-- 杭州口岸订单申报 -->
                        <entry key="IMPORTORDER">
                            <map>
                                <entry key="service_name" value="IMPORTORDER"/>
                                <entry key="msg_type" value="IMPORTORDER"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.port.hangzhou.HzPortOrderBillDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 杭州口岸电商企业代理支付企业进行支付单申报 -->
                        <entry key="PAYMENT">
                            <map>
                                <entry key="service_name" value="PAYMENT"/>
                                <entry key="msg_type" value="PAYMENT"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.port.hangzhou.HzPortPayBillDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 杭州口岸商品备案 -->
                        <entry key="PRODUCT_RECORD">
                            <map>
                                <entry key="service_name" value="PRODUCT_RECORD"/>
                                <entry key="msg_type" value="PRODUCT_RECORD"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.port.hangzhou.HzPortProductDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>
                <!-- 广州(海关总署) -->
                <entry key="GZPORT">
                    <map>
                        <!-- 广州(海关总署)口岸订单申报 -->
                        <entry key="IMPORTORDER">
                            <map>
                                <entry key="service_name" value="IMPORTORDER"/>
                                <entry key="msg_type" value="IMPORTORDER"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.port.customsoffice.CebPortOrderBillDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 平潭 -->
                <entry key="PTPORT">
                    <map>
                        <!-- 平潭订单申报  -->
                        <entry key="IMPORTORDER">
                            <map>
                                <entry key="service_name" value="IMPORTORDER"/>
                                <entry key="msg_type" value="IMPORTORDER"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.port.pingtan.PTPortOrderBillDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 宁波 -->
                <entry key="NBPORT">
                    <map>
                        <!-- 宁波订单申报 -->
                        <entry key="IMPORTORDER">
                            <map>
                                <entry key="service_name" value="IMPORTORDER"/>
                                <entry key="msg_type" value="IMPORTORDER"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.port.ningbo.NBPortOrderBillDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 空口岸 -->
                <entry key="EMPTYPORT">
                    <map>
                        <!-- 空口岸订单申报 -->
                        <entry key="IMPORTORDER">
                            <map>
                                <entry key="service_name" value="IMPORTORDER"/>
                                <entry key="msg_type" value="IMPORTORDER"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.port.empty.NullPortOrderBillDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>
            </map>
        </property>

        <property name="payMapping">
            <map>
                <!-- 支付宝外卡支付 -->
                <entry key="ALIPAY">
                    <map>
                        <!-- 支付单申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.alipay.AlipayOrderDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 支付单重新申报 -->
                        <entry key="PAYBILL_REDECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_REDECLAR"/>
                                <entry key="msg_type" value="PAYBILL_REDECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.alipay.AlipayOrderReDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 支付单申报状态查询 -->
                        <entry key="PAYBILL_QUERY">
                            <map>
                                <entry key="service_name" value="PAYBILL_QUERY"/>
                                <entry key="msg_type" value="PAYBILL_QUERY"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.alipay.AlipayPayOrderQueryCommand"/>
                            </map>
                        </entry>

                    </map>
                </entry>

                <!-- 易汇金支付 -->
                <entry key="YIHUIJINPAY">
                    <map>
                        <!-- 支付单申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.yihuijin.YihuijinPayOrderDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 支付单申报状态查询 -->
                        <entry key="PAYBILL_QUERY">
                            <map>
                                <entry key="service_name" value="PAYBILL_QUERY"/>
                                <entry key="msg_type" value="PAYBILL_QUERY"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.yihuijin.YihuijinPayOrderQueryCommand"/>
                            </map>
                        </entry>

                        <!-- 支付单重新申报 -->
                        <entry key="PAYBILL_REDECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_REDECLAR"/>
                                <entry key="msg_type" value="PAYBILL_REDECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.yihuijin.YihuijinPayOrderReDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 易汇金支付 -->
                <entry key="NEWYIHUIJINPAY">
                    <map>
                        <!-- 支付单申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.yihuijin.NewYihuijinPayOrderDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 易极付支付 -->
                <entry key="YIJIFUPAY">
                    <map>
                        <!-- 支付单申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.yijifu.YijifuPayOrderDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 宝付支付 -->
                <entry key="BAOFOOPAY">
                    <map>
                        <!-- 支付单申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.baofoo.BaofooPayOrderDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 财付通外卡支付 -->
                <entry key="TENPAY">
                    <map>
                        <!-- 财付通支付申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.tencent.TenPayOrderDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 财付通支付单申报状态查询 -->
                        <entry key="PAYBILL_QUERY">
                            <map>
                                <entry key="service_name" value="PAYBILL_QUERY"/>
                                <entry key="msg_type" value="PAYBILL_QUERY"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.tencent.TenPayOrderQueryCommand"/>
                            </map>
                        </entry>

                        <!-- 支付单重新申报 -->
                        <entry key="PAYBILL_REDECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_REDECLAR"/>
                                <entry key="msg_type" value="PAYBILL_REDECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.tencent.TenPayOrderReDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 财付通内卡支付 -->
                <entry key="TENPAY_INTL">
                    <map>
                        <!-- 财付通支付申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.tencent.TenPayOrderDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 财付通支付单申报状态查询 -->
                        <entry key="PAYBILL_QUERY">
                            <map>
                                <entry key="service_name" value="PAYBILL_QUERY"/>
                                <entry key="msg_type" value="PAYBILL_QUERY"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.tencent.TenPayOrderQueryCommand"/>
                            </map>
                        </entry>

                        <!-- 支付单重新申报 -->
                        <entry key="PAYBILL_REDECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_REDECLAR"/>
                                <entry key="msg_type" value="PAYBILL_REDECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.tencent.TenPayOrderReDeclareCommand"/>
                            </map>
                        </entry>
                    </map>
                </entry>

                <!-- 平潭支付 -->
                <entry key="PTPAY">
                    <map>
                        <!-- 支付单申报 -->
                        <entry key="PAYBILL_DECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_DECLAR"/>
                                <entry key="msg_type" value="PAYBILL_DECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.ptpay.PTPayOrderDeclareCommand"/>
                            </map>
                        </entry>

                        <!-- 支付单重新申报 -->
                        <entry key="PAYBILL_REDECLAR">
                            <map>
                                <entry key="service_name" value="PAYBILL_REDECLAR"/>
                                <entry key="msg_type" value="PAYBILL_REDECLAR"/>
                                <entry key="class_name"
                                       value="com.sfebiz.supplychain.provider.command.send.pay.ptpay.PTPayOrderDeclareCommand"/>
                            </map>
                        </entry>


                    </map>
                </entry>

            </map>
        </property>

    </bean>

</beans>
