<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sfebiz.supplychain.persistence.base.common.dao.NationDao">

<resultMap id="nationDOResult" type="com.sfebiz.supplychain.persistence.base.common.domain.NationDO">
    <result column="nation_e" property="nationE"/>
    <result column="nation" property="nation"/>
    <result column="ca_code" property="caCode"/>
    <result column="code" property="code"/>
    <result column="id" property="id"/>
    <result column="gmt_create" property="gmtCreate"/>
    <result column="gmt_modified" property="gmtModified"/>
</resultMap>

<sql id="nationDO_columns">
    nation_e,
    nation,
    ca_code,
    code,
    id,
    gmt_create,
    gmt_modified
</sql>

<sql id="nationDO_sqlForInsert">
    nation_e,
    nation,
    ca_code,
    code,
    id,
    gmt_create,
    gmt_modified
</sql>

<sql id="nationDO_columnsForInsert">
    #{nationE},
    #{nation},
    #{caCode},
    #{code},
    #{id},
    now(),
    now()
</sql>

<sql id="nationDO_setterForUpdate">
    <set>
        gmt_modified = now(),
        <if test="nationE != null">
            nation_e = #{nationE},
        </if>
        <if test="nation != null">
            nation = #{nation},
        </if>
        <if test="caCode != null">
            ca_code = #{caCode},
        </if>
        <if test="code != null">
            code = #{code},
        </if>
    </set>
</sql>

<sql id="nationDO_selector">
    select
    <include refid="nationDO_columns"/>
    from base_nation
</sql>

<sql id="nationDO_query_segment">
    <trim prefix="WHERE" prefixOverrides="AND|OR">
        <if test="data.nationE != null">
            AND nation_e = #{data.nationE}
        </if>
        <if test="data.nation != null">
            AND nation = #{data.nation}
        </if>
        <if test="data.caCode != null">
            AND ca_code = #{data.caCode}
        </if>
        <if test="data.code != null">
            AND code = #{data.code}
        </if>
        <if test="data.id != null">
            AND id = #{data.id}
        </if>
        <if test="data.gmtCreate != null">
            AND gmt_create = #{data.gmtCreate}
        </if>
        <if test="data.gmtModified != null">
            AND gmt_modified = #{data.gmtModified}
        </if>
        <if test="ins != null and ins != ''">
            AND
            <foreach collection="ins" item="item" open="(" separator="and" close=")">
                ${item.column} in
                <foreach collection="item.values" item="item" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </foreach>
        </if>
        <if test="notIns != null and notIns != ''">
            AND
            <foreach collection="notIns" item="item" open="(" separator="and" close=")">
                ${item.column} not in
                <foreach collection="item.values" item="item" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </foreach>
        </if>
        <if test="equals != null and equals != ''">
            AND
            <foreach collection="equals" item="item" open="(" separator="and" close=")">
                ${item.column} = #{item.value}
            </foreach>
        </if>
        <if test="notEquals != null and notEquals != ''">
            AND
            <foreach collection="notEquals" item="item" open="(" separator="and" close=")">
                ${item.column} = #{item.value}
            </foreach>
        </if>
        <if test="lts != null and lts != ''">
            AND
            <foreach collection="lts" item="item" open="(" separator="and" close=")">
                ${item.column} <![CDATA[ < ]]> #{item.value}
            </foreach>
        </if>
        <if test="gts != null and gts != ''">
            AND
            <foreach collection="gts" item="item" open="(" separator="and" close=")">
                ${item.column} <![CDATA[ > ]]> #{item.value}
            </foreach>
        </if>
        <if test="likes != null and likes != ''">
            AND
            <foreach collection="likes" item="item" open="(" separator="and" close=")">
                ${item.column} like concat('%', #{item.value}, '%')
            </foreach>
        </if>
        <if test="ltes != null and ltes != ''">
            AND
            <foreach collection="ltes" item="item" open="(" separator="and" close=")">
                ${item.column} <![CDATA[ <= ]]> #{item.value}
            </foreach>
        </if>
        <if test="gtes != null and gtes != ''">
            AND
            <foreach collection="gtes" item="item" open="(" separator="and" close=")">
                ${item.column} <![CDATA[ >= ]]> #{item.value}
            </foreach>
        </if>
        <if test="ranges != null and ranges != ''">
            AND
            <foreach collection="ranges" item="item" open="(" separator="and" close=")">
                ${item.column} <![CDATA[ >= ]]> #{item.from} and ${item.column} <![CDATA[ < ]]> #{item.to}
            </foreach>
        </if>
        AND is_delete = 0
    </trim>
</sql>


<insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="com.sfebiz.supplychain.persistence.base.common.domain.NationDO">
    insert into base_nation
    (
        <include refid="nationDO_sqlForInsert"/>
        ,is_delete
    )
    values
    (
        <include refid="nationDO_columnsForInsert"/>
        ,0
    )
</insert>

<update id="update" parameterType="com.sfebiz.supplychain.persistence.base.common.domain.NationDO">
    update base_nation
    <include refid="nationDO_setterForUpdate" />
    where id = #{id}
    and is_delete = 0
</update>


<update id="updateByQuery" parameterType="com.sfebiz.supplychain.persistence.base.common.domain.NationDO">
    update base_nation
    <set>
        gmt_modified = now(),
        <if test="update.nationE != null">
            nation_e = #{update.nationE},
        </if>
        <if test="update.nation != null">
            nation = #{update.nation},
        </if>
        <if test="update.caCode != null">
            ca_code = #{update.caCode},
        </if>
        <if test="update.code != null">
            code = #{update.code},
        </if>
    </set>
    <include refid="nationDO_query_segment"/>
</update>


<insert id="insertOrUpdate" useGeneratedKeys="true" keyProperty="id" parameterType="com.sfebiz.supplychain.persistence.base.common.domain.NationDO">
    insert into base_nation
    (
        <include refid="nationDO_sqlForInsert"/>
        ,is_delete
    )
    values
    (
        <include refid="nationDO_columnsForInsert"/>
        ,0
    )
    on duplicate key update
    gmt_modified = now()
    ,is_delete = 0
    <if test="nationE != null">
        ,nation_e = #{nationE}
    </if>
    <if test="nation != null">
        ,nation = #{nation}
    </if>
    <if test="caCode != null">
        ,ca_code = #{caCode}
    </if>
    <if test="code != null">
        ,code = #{code}
    </if>
</insert>

<!-- Delete -->
<update id="delete">
    update base_nation
    set is_delete = 1, gmt_modified = now()
    <include refid="nationDO_query_segment"/>
</update>
<update id="deleteById">
    update base_nation
    set is_delete = 1, gmt_modified = now()
    where id = #{id}
</update>



<!-- query class -->
<sql id="nationDO_groupBy">
    <if test="groupBys != null and groupBys != ''">
        group by
        <foreach collection="groupBys" item="item" open="" separator="," close="">
            #{item}
        </foreach>
    </if>
</sql>

<sql id="nationDO_orderby">
    <if test="orderBys != null and orderBys != ''">
        order by
        <foreach collection="orderBys" item="item" open="" separator="," close="">
            ${item.column} ${item.direction}
        </foreach>
    </if>
</sql>


<select id="query" resultMap="nationDOResult" parameterType="com.sfebiz.supplychain.persistence.base.common.domain.NationDO">
    <include refid="nationDO_selector"/>
    <include refid="nationDO_query_segment"/>
    <include refid="nationDO_groupBy"/>
    <include refid="nationDO_orderby"/>
</select>

<select id="query4Page" resultMap="nationDOResult" parameterType="com.sfebiz.supplychain.persistence.base.common.domain.NationDO">
    <include refid="nationDO_selector"/>
    <include refid="nationDO_query_segment"/>
    <include refid="nationDO_groupBy"/>
    <include refid="nationDO_orderby"/>
    limit ${startRowForMysql}, ${pageSize}
</select>


<select id="count" resultType="java.lang.Long">
    SELECT COUNT(1) FROM base_nation
    <include refid="nationDO_query_segment"/>
</select>

<select id="getById" resultMap="nationDOResult">
    <include refid="nationDO_selector"/>
    where id = #{id}
    and is_delete = 0
</select>

<select id="getByIdForUpdate" resultMap="nationDOResult">
    <include refid="nationDO_selector"/>
    where id = #{id}
    and is_delete = 0
    for update
</select>

</mapper>
